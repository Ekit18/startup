{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/startup/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/startup/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/startup",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "car-service": {
      "type": "application",
      "root": "apps/car-service",
      "entryFile": "main",
      "sourceRoot": "apps/car-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/car-service/tsconfig.app.json"
      }
    },
    "parts": {
      "type": "application",
      "root": "apps/parts",
      "entryFile": "main",
      "sourceRoot": "apps/parts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/parts/tsconfig.app.json"
      }
    },
    "chat-gpt": {
      "type": "application",
      "root": "apps/chat-gpt",
      "entryFile": "main",
      "sourceRoot": "apps/chat-gpt/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-gpt/tsconfig.app.json"
      }
    },
    "car": {
      "type": "application",
      "root": "apps/car",
      "entryFile": "main",
      "sourceRoot": "apps/car/src",
      "compilerOptions": {
        "tsConfigPath": "apps/car/tsconfig.app.json"
      }
    },
    "aws": {
      "type": "application",
      "root": "apps/aws",
      "entryFile": "main",
      "sourceRoot": "apps/aws/src",
      "compilerOptions": {
        "tsConfigPath": "apps/aws/tsconfig.app.json"
      }
    },
    "testing": {
      "type": "application",
      "root": "apps/testing",
      "entryFile": "main",
      "sourceRoot": "apps/testing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/testing/tsconfig.app.json"
      }
    }
  }
}